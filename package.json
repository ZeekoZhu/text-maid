{
  "name": "text-maid",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@asciidoctor/core": "^2.0.3",
    "@godaddy/terminus": "^4.2.0",
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/terminus": "^6.5.2",
    "@types/express": "^4.17.1",
    "@zeeko/markdown-it-presets": "0.0.3",
    "asciidoctor-highlight.js": "^0.2.0",
    "asciidoctor-katex": "^0.3.0",
    "asciidoctor.js": "^1.5.9",
    "cheerio": "^1.0.0-rc.3",
    "highlight.js": "^9.15.10",
    "katex": "^0.11.1",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^5.2.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "uslug": "^1.0.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/cheerio": "^0.22.13",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^24.0.18",
    "@types/katex": "0.11.0",
    "@types/lodash": "^4.14.144",
    "@types/markdown-it": "^0.0.9",
    "@types/markdown-it-anchor": "^4.0.3",
    "@types/markdown-it-container": "^2.0.2",
    "@types/node": "13.1.4",
    "@types/prettier": "^1.18.3",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.8",
    "@types/uslug": "^1.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
